/* miniprogram/pages/index/index.wxss */
@font-face {
  font-family: 'iconfont';
  src: url('data:font/truetype;charset=utf-8;base64,') format('truetype');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-sousuo:before {
  content: "\e688";
}
.icon-shouye:before {
  content: "\e619";
}
.icon-dingwei:before {
  content: "\e604";
}
.icon-dianhua:before {
  content: "\e60c";
}
.icon-xuexiao:before {
  content: "\e610";
}
.mapNavShow {
  visibility: hidden;
  z-index: 0;
}
.container {
  height: 100vh;
}
.container .map-nav {
  width: 100%;
  height: 536rpx;
}
.container .map-nav_wrapper {
  height: 100rpx;
  background-color: #fff;
  font-size: 28rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  position: relative;
}
.container .map-nav_wrapper .map-nav-title {
  display: flex;
  align-items: center;
  position: absolute;
  left: 0;
  width: 40%;
  height: 100%;
  line-height: 100rpx;
  padding-left: 20rpx;
}
.container .map-nav_wrapper .map-nav-title_name {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.container .map-nav_wrapper .map-nav-search {
  height: 60rpx;
  width: 200rpx;
  margin: 0 20rpx;
  border-radius: 30rpx;
  background-color: #F4F5F7;
  font-size: 28rpx;
  transition: width 0.2s linear;
}
.container .map-nav_wrapper .map-nav-search_wrapper {
  width: 90%;
  margin: 0 auto;
  color: #BEBEBE;
}
.container .map-nav_wrapper .map-nav-search .iconfont {
  font-size: 1.2rem;
  display: inline-block;
  height: 60rpx;
  line-height: 60rpx;
}
.container .map-nav_wrapper .map-nav-search .icon-sousuo {
  position: absolute;
}
.container .map-nav_wrapper .map-nav-search_input {
  position: relative;
  display: inline-block;
  height: 60rpx;
  line-height: 60rpx;
  width: 90%;
  padding-left: 48rpx;
}
.container .map-content {
  margin-top: 15rpx;
  transition: all 0.3s linear;
  background-color: #F2F2F2;
  position: relative;
}
.container .map-content .map-control {
  color: #929292;
  background: #fff;
  text-align: center;
  padding: 16rpx 0;
  font-size: 28rpx;
  box-shadow: 0px 2rpx 1rpx rgba(0, 0, 0, 0.1);
}
.container .map-content .map-control-span {
  display: inline-block;
  height: 40rpx;
  line-height: 40rpx;
}
.container .map-content .scrollContent {
  height: 100%;
}
.container .map-content .scrollContent .content-wrapper {
  padding: 30rpx 40rpx 0;
}
.container .map-content .scrollContent .content-wrapper .card-wrapper {
  height: 250rpx;
  width: 100%;
  border: 2rpx solid #64B5F6;
  border-radius: 10rpx;
  background: #fff;
}
.container .map-content .scrollContent .content-wrapper .card-wrapper .main-card {
  padding: 35rpx 30rpx 30rpx;
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: row;
  position: relative;
}
.container .map-content .scrollContent .content-wrapper .card-wrapper .main-card_address {
  flex: 0 0 60%;
  width: 60%;
}
.container .map-content .scrollContent .content-wrapper .card-wrapper .main-card_address-title {
  height: 84rpx;
}
.container .map-content .scrollContent .content-wrapper .card-wrapper .main-card_address-title .icon-xuexiao {
  font-size: 1.2rem;
  color: #bebebe;
  display: inline-block;
  position: absolute;
  vertical-align: middle;
}
.container .map-content .scrollContent .content-wrapper .card-wrapper .main-card_address-title .title {
  display: inline-block;
  color: rgba(0, 0, 0, 0.8);
  font-size: 32rpx;
  font-weight: bold;
  letter-spacing: 2rpx;
  padding-left: 45rpx;
}
.container .map-content .scrollContent .content-wrapper .card-wrapper .main-card_address-detail {
  font-size: 28rpx;
  margin-top: 20rpx;
  color: #9b9b9b;
  height: 84rpx;
}
.container .map-content .scrollContent .content-wrapper .card-wrapper .main-card_address-detail .icon-shouye {
  font-size: 0.9rem;
  color: #9b9b9b;
  display: inline-block;
  margin-top: 2rpx;
  position: absolute;
}
.container .map-content .scrollContent .content-wrapper .card-wrapper .main-card_address-detail .title {
  display: inline-block;
  letter-spacing: 1rpx;
  padding-left: 30rpx;
  height: 100%;
  -webkit-line-clamp: 2;
  /*用来限制在一个块元素显示的文本的行数*/
  display: -webkit-box;
  /*必须结合的属性，将对象作为弹性伸缩盒子模型显示*/
  -webkit-box-orient: vertical;
  /*必须结合的属性 ，设置或检索伸缩盒对象的子元素的排列方式*/
  overflow: hidden;
  word-break: break-all;
}
.container .map-content .scrollContent .content-wrapper .card-wrapper .main-card_contact {
  flex: 1;
  margin-left: 30rpx;
  padding: 15rpx 30rpx;
  border-left: 1rpx solid rgba(0, 0, 0, 0.2);
  text-align: center;
}
.container .map-content .scrollContent .content-wrapper .card-wrapper .main-card_contact-distance {
  font-size: 28rpx;
  color: #64B5F6;
}
.container .map-content .scrollContent .content-wrapper .card-wrapper .main-card_contact-order {
  margin: 18rpx auto;
}
.container .map-content .scrollContent .content-wrapper .card-wrapper .main-card_contact-iconWrapper {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
}
.container .map-content .scrollContent .content-wrapper .card-wrapper .main-card_contact-icon {
  background-color: #f4f5f7;
  border-radius: 50%;
  padding: 9rpx 10rpx;
}
.container .map-content .scrollContent .content-wrapper .card-wrapper:not(:nth-child(1)) {
  margin-top: 30rpx;
}
