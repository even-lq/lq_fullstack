/* miniprogram/pages/index/index.wxss */
@font-face {
  font-family: 'iconfont';
  src: url('data:font/truetype;charset=utf-8;base64,') format('truetype');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-sousuo:before {
  content: "\e688";
}
.mapNavShow {
  visibility: hidden;
  z-index: 0;
}
.map-nav {
  width: 100%;
  height: 536rpx;
}
.map-nav_wrapper {
  height: 100rpx;
  background-color: #fff;
  font-size: 28rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  position: relative;
}
.map-nav_wrapper .map-nav-title {
  display: flex;
  align-items: center;
  position: absolute;
  left: 0;
  width: 40%;
  height: 100%;
  line-height: 100rpx;
  padding-left: 20rpx;
}
.map-nav_wrapper .map-nav-title_name {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.map-nav_wrapper .map-nav-search {
  height: 60rpx;
  width: 200rpx;
  margin: 0 20rpx;
  border-radius: 30rpx;
  background-color: #F4F5F7;
  font-size: 28rpx;
  transition: width 0.2s linear;
}
.map-nav_wrapper .map-nav-search_wrapper {
  width: 90%;
  margin: 0 auto;
  color: #BEBEBE;
}
.map-nav_wrapper .map-nav-search .iconfont {
  font-size: 1.2rem;
  display: inline-block;
  height: 60rpx;
  line-height: 60rpx;
}
.map-nav_wrapper .map-nav-search .icon-sousuo {
  position: absolute;
}
.map-nav_wrapper .map-nav-search_input {
  position: relative;
  display: inline-block;
  height: 60rpx;
  line-height: 60rpx;
  width: 90%;
  padding-left: 48rpx;
}
.map-content {
  margin-top: 15rpx;
  transition: all 0.3s linear;
  background-color: #F2F2F2;
  position: relative;
}
.map-content .map-control {
  color: #929292;
  background: #fff;
  text-align: center;
  padding: 16rpx 0;
  font-size: 28rpx;
  box-shadow: 0px 2rpx 1rpx rgba(0, 0, 0, 0.1);
}
.map-content .map-control-span {
  display: inline-block;
  height: 40rpx;
  line-height: 40rpx;
}
.map-content .scrollContent {
  height: 100%;
}
