.border-line {
  content: '';
  display: block;
  height: 2rpx;
  background-color: #eeee;
  position: absolute;
  bottom: 0;
  left: 30rpx;
  right: 30rpx;
}
.border-none {
  content: '';
  display: block;
  height: 0rpx;
}
.border-right-line {
  content: '';
  display: block;
  width: 2rpx;
  background-color: #eeee;
  position: absolute;
  top: 40rpx;
  left: 100%;
  bottom: 40rpx;
}
.border-right-none {
  content: '';
  display: block;
  width: 0rpx;
}
.campus-order__mine {
  min-height: calc(100vh - 110rpx);
}
.campus-order__mine-bgImg {
  position: fixed;
  z-index: -1;
  top: 0;
  left: 0;
  right: 0;
  width: 100%;
  height: 485rpx;
  background-image: url('data:image/jpeg;base64,');
  background-position: center center;
  background-size: cover;
  background-repeat: no-repeat;
}
.campus-order__mine-wrapper {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.campus-order__mine-wrapper .campus-order__mine-profile {
  flex: 130rpx;
  position: relative;
  width: 100%;
  padding: 0 20rpx;
}
.campus-order__mine-wrapper .campus-order__mine-profile::after {
  content: '';
  display: block;
  height: 2rpx;
  background-color: #eeee;
  position: absolute;
  bottom: 0;
  left: 30rpx;
  right: 30rpx;
}
.campus-order__mine-wrapper .campus-order__mine-profile:last-child::after {
  content: '';
  display: block;
  height: 0rpx;
}
.campus-order__mine-wrapper .campus-order__mine-profile .avatar {
  position: absolute;
  top: -35rpx;
  width: 120rpx;
  height: 120rpx;
  display: inline-block;
  padding: 2rpx;
  margin-left: 25rpx;
  border-radius: 50%;
  background-color: #fff;
}
.campus-order__mine-wrapper .campus-order__mine-profile .avatar .img {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
}
.campus-order__mine-wrapper .campus-order__mine-profile .nickname {
  display: inline-block;
  height: 85rpx;
  line-height: 85rpx;
  font-size: 32rpx;
  font-weight: 700;
  letter-spacing: 2rpx;
  margin-left: 175rpx;
}
.campus-order__mine-wrapper .campus-order__mine-profile.self-height {
  flex: 0 0 160rpx;
}
.campus-order__mine-wrapper .campus-order__mine-profile.self-flex {
  display: flex;
  justify-content: space-around;
}
.campus-order__mine-wrapper .campus-order__mine-profile_column {
  position: relative;
  flex: 1;
  width: 100%;
  height: 160rpx;
  text-align: center;
}
.campus-order__mine-wrapper .campus-order__mine-profile_column::after {
  content: '';
  display: block;
  width: 2rpx;
  background-color: #eeee;
  position: absolute;
  top: 40rpx;
  left: 100%;
  bottom: 40rpx;
}
.campus-order__mine-wrapper .campus-order__mine-profile_column:last-child::after {
  content: '';
  display: block;
  width: 0rpx;
}
.campus-order__mine-wrapper .campus-order__mine-profile_column .value,
.campus-order__mine-wrapper .campus-order__mine-profile_column .key {
  height: 80rpx;
  line-height: 60rpx;
  box-sizing: border-box;
}
.campus-order__mine-wrapper .campus-order__mine-profile_column .value {
  padding-top: 20rpx;
  font-size: 36rpx;
  font-weight: 700;
  color: #2ba4f5;
}
.campus-order__mine-wrapper .campus-order__mine-profile_column .key {
  padding-bottom: 20rpx;
}
